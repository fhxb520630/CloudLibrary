<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.cloudlibrary"
    >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />



    <application
        tools:replace="android:icon"
        android:name=".CLApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon1"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:launchMode="singleInstance"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".InformActivity"></activity>
        <activity android:name=".LogActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".PosActivity" />
        <activity android:name=".WebActivityNews" />
        <activity android:name=".GradeActivity"/>
        <activity android:name=".GradeManagerActivity"/>
        <activity android:name=".CommentContentActivity"/>
        <activity android:name=".FullImageActivity"/>
        <activity android:name=".WelComeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>

    </application>

</manifest>